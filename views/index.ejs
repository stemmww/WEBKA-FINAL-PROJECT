                <h2>User List</h2>
            <table border="1">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Age</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(user => { %>
                        <tr>
                            <td><%= user.name %></td>
                            <td><%= user.email %></td>
                            <td><%= user.age %></td>
                            <td>
                                <form method="GET" action="/users/update/<%= user._id %>" style="display:inline;">
                                    <button type="submit">Edit</button>
                                </form>
                                <form method="POST" action="/users/delete/<%= user._id %>" style="display:inline;">
                                    <button type="submit" onclick="return confirm('Are you sure?')">Delete</button>
                                </form>
                            </td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
